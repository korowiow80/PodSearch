		$(".visionSlider a").click(function(e){
			e.preventDefault();
			
			slideWidth = parseInt(windowWidth) + parseInt(contentWidth);
			$(".content").children().animate({
				marginLeft: slideWidth + "px"
			}, 500);
			$(".vision, .visionShort, .visionCollapsed").animate({
				width: "95%"
			}, 500, function(){
				window.location.href = vision;
			});
		});
		
		$("#vision_click, #vision_click_home").click(function(e){
			e.preventDefault();
			$("body").css("background-color","#efeeee");
			slideWidth = parseInt(windowWidth) + parseInt(contentWidth);
			$(".content").children().animate({
				marginLeft: slideWidth + "px"
			}, 500);
			$(".vision, .visionShort, .visionCollapsed").animate({
				width: "95%"
			}, 500, function(){
				window.location.href = vision;
			});
		});
		
		$(".liveSlider a").click(function(e){
			e.preventDefault();
			slideWidth = parseInt(windowWidth) + parseInt(contentWidth);
			//create logo transparency for slide on Live link (remove image/bottom border)
			$('#logo_link').addClass('logoClear');
			$('.header #logoWhite').remove();

			$(".content").children().animate({
				marginLeft: "-" + slideWidth + "px"
			}, 500);
			$(".vision, .visionFull, .visionCollapsed").animate({
				width: "5%"
			}, 500, function(){
				window.location.href = live;
			});
		});
		
		$("#live_click, #live_click_home").click(function(e){
			e.preventDefault();
			//create logo transparency for slide on Live link (remove image/bottom border)
