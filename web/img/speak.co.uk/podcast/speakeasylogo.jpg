			$('.header #logoWhite').remove();
			
			slideWidth = parseInt(windowWidth) + parseInt(contentWidth);
			$(".content").children().animate({
				marginLeft: "-" + slideWidth + "px"
			}, 500);
			$(".vision, .visionShort, .visionCollapsed").animate({
				width: "5%"
			}, 500, function(){
				window.location.href = live;
			});
			
		});

	

		$("#mycarousel").jcarousel(); //initiate carousel
		
	});
